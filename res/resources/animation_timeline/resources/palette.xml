		<controlIndicator> #323232 </controlIndicator>
		<controlIndicatorHevered> #666666 </controlIndicatorHevered>
		<controlFrame> #A7B2B8 </controlFrame>
		<controlDecor> #999999 </controlDecor>
		<controlDecorAlt> #33336E </controlDecorAlt>
		<toolButtonBackground> #0F0F0F </toolButtonBackground>
		<breadcrumbBackground> #FFFFFF </breadcrumbBackground>
		<windowBase> #CDD2D6 </windowBase>
		<windowTitlebar> #A7B2B8 </windowTitlebar>
