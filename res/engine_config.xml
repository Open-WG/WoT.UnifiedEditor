	<logging>
		<!--
			This section controls the various settings for client log output
		-->
		<!--
			Suppress client logging of various categories of log output.
			By setting priority you can alter minimum priority level of a message
			(by default all priorities are suppressed)
		-->
		<suppress>
			<categories>
				<!--
				<category>
					Scaleform
					<priority> INFO </priority>
				</category>
				-->
			</categories>
		</suppress>
		<!--
			If enabled, warning and error messages will be logged to the
			MessageLoggers. In order to use this variable you should set the
			UID environment variable to the Unix UID so that usernames match.
		-->
		<logToMessageLogger> false </logToMessageLogger>
		<!-- The ID used by the process when registering with MessageLoggers.
			If this ID does not match a MessageLogger's filter, the process
			will not log to that MessageLogger. The value range is 0-255. -->
		<loggerID> 111 </loggerID>
		<!-- fake ID, disables logging from clients -->
		<file>
			game.log
			<!--if name is not set, then the executable_file_name.log will be used.-->
			<enabled> true </enabled>
			<!--if not set, then default as false-->
			<openMode> a </openMode>
			<!--possible value: "a"(append)  or "w"(overwrite), if not set, then default as "a"-->
			<maxSizeKB> 2048 </maxSizeKB>
			<!--if not set, then 0 don't trim-->
			<trimToKB> 512 </trimToKB>
			<!--enable timestamps-->
			<showTime> true </showTime>
			<!--activate flushing after each write-->
			<disableBuffering> false </disableBuffering>
			<!--length of message buffer. default value is 8192 -->
			<messageLength> 8192 </messageLength>
		</file>
		<file>
			python.log
			<!--if name is not set,, then the executable_file_name.log will be used.-->
			<enabled> true </enabled>
			<!--if name is not set, then default as false-->
			<!--possible values: TRACE,DEBUG,INFO,NOTICE,WARNING,ERROR,CRITICAL,HACK,ASSET, if not set, then ALL will be included -->
			<!-- <severity> TRACE </severity> -->
			<severity> DEBUG </severity>
			<severity> INFO </severity>
			<severity> NOTICE </severity>
			<severity> WARNING </severity>
			<severity> ERROR </severity>
			<severity> CRITICAL </severity>
			<severity> HACK </severity>
			<!--category> Chunk</category>only one category is allowed, if not set, then all categories will be included.-->
			<source> SCRIPT </source>
			<source> UI </source>
			<!--possible values: CPP, SCRIPT, if not set, then ALL will be included-->
			<openMode> a </openMode>
			<!--possible value: "a"(append)  or "w"(overwrite), if not set, then default as "a"-->
			<maxSizeKB> 2048 </maxSizeKB>
			<trimToKB> 512 </trimToKB>
			<!--enable timestamps-->
			<showTime> true </showTime>
			<!--activate flushing after each write-->
			<disableBuffering> false </disableBuffering>
			<!--length of message buffer. default value is 8192 -->
			<messageLength> 8192 </messageLength>
		</file>
		<file>
			ui.log
			<enabled> true </enabled>
			<severity> TRACE </severity>
			<severity> DEBUG </severity>
			<severity> INFO </severity>
			<severity> WARNING </severity>
			<severity> ERROR </severity>
			<severity> CRITICAL </severity>
			<source> UI </source>
			<openMode> a </openMode>
			<maxSizeKB> 2048 </maxSizeKB>
			<trimToKB> 512 </trimToKB>
			<!--enable timestamps-->
			<showTime> true </showTime>
			<!--activate flushing after each write-->
			<disableBuffering> false </disableBuffering>
			<!--length of message buffer. default value is 8192 -->
			<messageLength> 8192 </messageLength>
		</file>
	</logging>
	<webBrowser>
		<processExe> cef_browser_process </processExe>
		<cachePath> profile/cef_cache </cachePath>
		<resourcePath> cef </resourcePath>
		<!-- Enable CEF HW Acceleration with using shared texture feature for d3d11 devices -->
		<hardwareAcceleration> true </hardwareAcceleration>
		<logFile> cef.log </logFile>
		<!-- "verbose", "info", "warning", "error", "disable" -->
		<logVerbosity> disable </logVerbosity>
		<enableFlash> true </enableFlash>
		<!-- Should SSL certificate errors be ignored (dev build only) -->
		<ignoreCertErrors> false </ignoreCertErrors>
		<!-- -1 is disabled, must be between 1024 and 65535 -->
		<debugPort> -1 </debugPort>
		<!-- 0 is default -->
		<maxFrameRate> 60 </maxFrameRate>
		<!-- Comma delimited ordered, list of language codes without any whitespace used in "Accept-Language" HTTP header -->
		<acceptLangList>
		</acceptLangList>
		<!-- locale used by the browser, uses same language codes as acceptLangList -->
		<locale>
		</locale>
		<!-- Enable video auto play. Works only in new CEF versions(61+) -->
		<autoPlayVideo> true </autoPlayVideo>
	</webBrowser>
	<gameface>
		<taskSystem>
			<!-- Number of threads for resources and layout works are computed as -->
			<!-- threadsCount = min(maxThreads, int(cpuCores / cpuCoresThreshold) + 1) -->
			<maxThreads>2</maxThreads>
			<cpuCoresThreshold>5</cpuCoresThreshold>
		</taskSystem>
	</gameface>
	<visualScript>
		<debuggerPort>    1454    </debuggerPort>
	</visualScript>
