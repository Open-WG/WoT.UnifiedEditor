			<item label="Destroy self 7" />
			<item label="Kill engine 8" />
			<item label="Damage self ammo bay 9" />
			<item label="Damage self fuel tank 0" />
			<item label="Damage self engine -" />
			<item label=" " />
			<item label="Navigate to shot point C" />
			<item label="Toggle client filters F" />	
			<item label="Cruise control forward G" />
			<item label="Respawn self K" />
			<item label="Toggle teleport N" />
			<item label="Roll self O" />
			<item label="Capture closest base P" />
			<item label="Teleport to shot point Q" />
			<item label="Back self on tracks R" />
			<item label="Log tkill ratings T" />
			<item label="Set signal V" />
			<item label="Toggle pause AI 'PAUSE'" />
			<item label="Kill all enemies 'BACKSLASH'" />
			<item label="  " />			
			<item label="Toggle wide view F1" />
			<item label="Toggle avatar fly fast F2" />
			<item label="Toggle avatar fly slow F3" />
			<item label="Screenshot F9" />
			<item label="GUI debug panel F10" />
		</item>
	</item>

	<item label="Switch Mode Selector Toolset" action="from gui.development.ui.mode_selector.mode_selector_toolset import showToolset; showToolset()"/>

	<item label="Change Window Size">
		<item label="Sizes" action="from gui.shared.utils.monitor_settings import g_monitorSettings as m; m.changeWindowSize">
			<param label="1024x768     Ctrl+1">(1024, 768)</param>
			<param label="1280x768     Ctrl+2">(1280, 768)</param>
			<param label="1366x768     Ctrl+3">(1366, 768)</param>
			<param label="1600x900     Ctrl+4">(1600, 900)</param>
			<param label="1920x1080   Ctrl+5">(1920, 1080)</param>
			<param label="2200x1200   Ctrl+6">(2200, 1200)</param>
			<param label="2560x1440   Ctrl+7">(2560, 1440)</param>
			<param label="3840x2160   Ctrl+8">(3840, 2160)</param>
		</item>
		<item label="Init hot keys Change Window Size manager" action="from development.as_tools.change_window_size_manager import g_change_manager as m; m.">
			<param label="start">start()</param>
			<param label="stop">stop()</param>
		</item>
	</item>

	<item label="Frontline">
			<item label="Frontline Notifications" action="$p.showFrontlineSysMessage">
				<param label="resetSkillsSuccess">(0)</param>
				<param label="financeGold">(1)</param>
				<param label="financeCredits">(2)</param>
				<param label="resetAllSuccess">(3)</param>
				<param label="notAvailable">(4)</param>
				<param label="againAvailable">(5)</param>
				<param label="sellBlanks">(6)</param>
				<param label="buyBlanks">(7)</param>
				<param label="victoryMessage">(8)</param>
			</item>
			<item label="Reserves">
				<item label="unlock reserves" action="$p.unlockFrontlineReserves()"/>
