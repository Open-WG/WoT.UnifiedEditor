<root>
	<item label="Unlock all vehicles" action="$unlockAll"/>
	<item label="Obtain all items" action="$obtainAll"/>
	<item label="Obtain vehicle" action="$obtainVehicle" input="true" format="('%s')"/>
	<item label="Set hangar" action="$hangar.setPath">
		<param>('hangar_v4')</param>
		<param>('h33_comp7')</param>
		<param>('hangar_personal_missions')</param>
	</item>

	<item label="Set money" action="$p.stats.setMoney">
		<param label="100000000">(100000000, 100000000, 100000000, 100000000)</param>
		<param label="0">(0, 0, 0, 0)</param>
	</item>

	<item label="Add BattlePass points">
		<item label="Standard amounts" action="$p.stats.addBattlePassPoints">
			<param label="50">(0, 50)</param>
			<param label="500">(0, 500)</param>
			<param label="1250">(0, 1250)</param>
			<param label="2500">(0, 2500)</param>
			<param label="5000">(0, 5000)</param>
			<param label="7500">(0, 7500)</param>
		</item>
		<item label="Custom amount" action="$p.stats.addBattlePassPoints(0, " input="true" format="%s)"/>
	</item>

	<item label="Set battlesCount" action="$p.stats.setDossierField('a15x15.battlesCount', " input="true" format="%s)"/>

	<item label="Premium Account">
		<item label="Leave blank for 24h"/>
		<item label="WoT Premium (seconds)" action="$p.stats.setPremium(2, " input="true" format="%s)"/>
		<item label="WG Premium (seconds)" action="$p.stats.setPremium(1, " input="true" format="%s)"/>
		<item label="Clear Premium" action="$p.stats.setPremium(2, 0); $p.stats.setPremium(1, 0)"/>
	</item>

	<item label="WoT+">
		<item label="Toggle WoT+" action="$wotPlusController.toggleWotPlusDev()" />
		<item label="Set WoT+ State Available" action="$wotPlusController.setWotPlusStateDev(0)" />
		<item label="Set WoT+ State Active" action="$wotPlusController.setWotPlusStateDev(1)" />
		<item label="Set WoT+ State Canceled" action="$wotPlusController.setWotPlusStateDev(2)" />
		<item label="Give next attendance reward" action="$wotPlusController.giveAttendanceRewardDev()" />
		<item label="Clear loadouts cache" action="from gui.development.ui import clearWotPlusAssistantCache; clearWotPlusAssistantCache()" />
		<item label="Request loadouts (with cache clear)" action="from gui.development.ui import invalidateWotPlusAssistant; invalidateWotPlusAssistant()" />
		<item label="Block from requests" action="from gui.development.ui import blockWotPlusAssistantFromRequests; blockWotPlusAssistantFromRequests()" />
	</item>


	<item label="Easy tank equip">
		<item label="Clear cache" action="from gui.development.ui import clearEasyTankEquipCache; clearEasyTankEquipCache()" />
		<item label="Request loadouts" action="from gui.development.ui import invalidateEasyTankEquip; invalidateEasyTankEquip()" />
		<item label="Block from requests" action="from gui.development.ui import blockEasyTankEquipFromRequests; blockEasyTankEquipFromRequests()" />
	</item>

	<item label="Prestige system">
		<item label="Update vehicle points" action="$cvi.intCD; $p.stats.updateVehiclePrestige(cvi.intCD, " input="true" format="%s)" />
	</item>

	<item label="Set herotank" action="$setHero">
		<param>('germany:G42_Maus')</param>
		<param>('japan:J27_O_I_120')</param>
		<param>('uk:GB94_Centurion_Mk5-1_RAAC')</param>
		<param>('ussr:R54_KV-5')</param>
		<param>('Reset')</param>>
	</item>

	<item label="[Event Platform] markers" action="$p.">
		<param label="add marker to avatar position">addOrRemoveMarkerTo(0)</param>
		<param label="add marker to avatar">addOrRemoveMarkerTo(2)</param>
		<param label="remove all marker">addOrRemoveMarkerTo(1)</param>
	</item>

	<item label="Dog Tags">
		<item label="In hangar">
			<item label="Unlock all components" action="$p.dogTags.unlockAllComponentsDev()"/>
		</item>
		<item label="In battle">
			<item label="Random victim Dog Tag" action="from avatar_helpers.dog_tags_helpers import DogTagsHelpers; DogTagsHelpers.showRandomVictimDogTag()"/>
			<item label="Random killer Dog Tag" action="from avatar_helpers.dog_tags_helpers import DogTagsHelpers; DogTagsHelpers.showRandomKillerDogTag()"/>
		</item>
	</item>

	<item label="Battle Notifier">
			<item label="ShowWin" action="$battle_notifier.showWin()"/>
			<item label="ShowDraw" action="$battle_notifier.showDraw()"/>
			<item label="ShowLose" action="$battle_notifier.showLose()"/>
			<item label="ShowQueuing" action="$battle_notifier.createQueue()"/>
	</item>

	<item label="Daily Quests">
			<item label="Complete regular quest" action="$p.stats.completeDailyQuest">
				<param label="easy">("easy")</param>
				<param label="medium">("medium")</param>
				<param label="hard">("hard")</param>
				<param label="bonus">("bonus")</param>
			</item>
			<item label="Reroll Quest" action="$p.stats.rerollDailyQuestDev">
				<param label="easy">("easy")</param>
				<param label="medium">("medium")</param>
				<param label="hard">("hard")</param>
				<param label="bonus">("bonus")</param>
			</item>
			<item label="Reset Reroll" action="$p.stats.resetRerollTimeout()"/>
			<item label="Complete premium quest" action="$p.stats.completePremiumDaily()"/>
			<item label="Set Epic Reward Tokens" action="$p.stats.setEpicRewardTokens" input="true" format="(%s)"/>
	</item>

	<item label="Weekly Quests">
		<item label="Reroll Weekly Quest By Id" action="$p.stats.rerollWeeklyQuestDev">
			<param label="1">(1)</param>
			<param label="2">(2)</param>
			<param label="3">(3)</param>
		</item>
		<item label="Complete Weekly Quest By Id" action="$p.stats.completeWeeklyQuestDev">
			<param label="1">(1)</param>
			<param label="2">(2)</param>
			<param label="3">(3)</param>
		</item>
		<item label="Reset Reroll Timeout" action="$p.stats.resetWeeklyQuestsRerollTimeout()"/>
		<item label="Trigger New Week Notification" action="$p.stats.weeklyQuestsNewWeek()"/>
		<item label="Reroll All Quests" action="$p.stats.weeklyQuestsRerollAll()"/>
	</item>

	<item label="PersonalReserves">
		<item label="Add a reserve by Id" action="$boosters.addBooster(" input="true" format="%s, 1)"/>
		<item label="Add standard reserves" action="$boosters.addBooster">
			<param label="xp">(121000, 10)</param>
			<param label="xp - premium">(121001, 10)</param>
			<param label="credits">(121002, 10)</param>
			<param label="credits - premium">(121003, 10)</param>
			<param label="crew+fxp">(121004, 10)</param>
			<param label="crew+fxp - premium">(121005, 10)</param>
			<param label="frontline xp">(111001, 10)</param>
		</item>
		<item label="Add expirable reserves" action="$boosters.addBooster">
			<param label="xp 7 days">(122210, 3)</param>
			<param label="xp 14 days">(122211, 3)</param>
			<param label="xp 28 days">(122212, 3)</param>
			<param label="xp - premium 7 days">(122310, 3)</param>
			<param label="xp - premium 14 days">(122311, 3)</param>
			<param label="xp - premium 28 days">(122312, 3)</param>
			<param label="credits 7 days">(122410, 3)</param>
			<param label="credits 14 days">(122411, 3)</param>
			<param label="credits 28 days">(122412, 3)</param>
			<param label="crew+fxp 7 days">(122510, 3)</param>
			<param label="crew+fxp 14 days">(122511, 3)</param>
			<param label="crew+fxp 28 days">(122512, 3)</param>
			<param label="crew+fxp - premium 7 days">(122610, 3)</param>
			<param label="crew+fxp - premium 14 days">(122611, 3)</param>
			<param label="crew+fxp - premium 28 days">(122612, 3)</param>
		</item>
		<item label="Add custom expirable reserves (expires in seconds)">
			<item label="These commands might cause incorrect behavior." />
			<item label="xp" action="$boosters.addExpiringBooster(122210, 3, " input="true" format="%s)"/>
			<item label="xp premium " action="$boosters.addExpiringBooster(122310, 3, " input="true" format="%s)"/>
			<item label="credits" action="$boosters.addExpiringBooster(122410, 3, " input="true" format="%s)"/>
			<item label="crew+fxp" action="$boosters.addExpiringBooster(122510, 3, " input="true" format="%s)"/>
			<item label="crew+fxp premium" action="$boosters.addExpiringBooster(122610, 3, " input="true" format="%s)"/>
		</item>
		<item label="Clear reserves" action="$boosters.clearBoosters()" />
		<item label="Activate clan reserves" action="$boosters.activateClanBooster">
			<param label="credits">(1)</param>
			<param label="credits_2.0">(13)</param>
			<param label="xp">(2)</param>
			<param label="xp_2.0">(15)</param>
			<param label="free_xp">(4)</param>
			<param label="free_xp_2.0">(14)</param>
			<param label="crew_xp">(3)</param>
			<param label="crew_xp_2.0">(16)</param>
		</item>
		<item label="Clear clan reserves" action="$boosters.deactivateClanBoosters()" />
		<item label="Reset account settings" action="$boosters.resetAccountSettings()" />
	</item>

	<item label="Shortcuts">
		<item label="In battle (CAPSLOCK+)...">
			<item label="Heel self 1" />
			<item label="Reload gun 2" />
			<item label="Set self on fire 3" />
			<item label="Explode self 4" />
			<item label="Break left track 5" />
			<item label="Break right track 6" />
			<item label="Destroy self 7" />
			<item label="Kill engine 8" />
			<item label="Damage self ammo bay 9" />
			<item label="Damage self fuel tank 0" />
			<item label="Damage self engine -" />
			<item label=" " />
			<item label="Navigate to shot point C" />
			<item label="Toggle client filters F" />	
			<item label="Cruise control forward G" />
			<item label="Respawn self K" />
			<item label="Toggle teleport N" />
			<item label="Roll self O" />
			<item label="Capture closest base P" />
			<item label="Teleport to shot point Q" />
			<item label="Back self on tracks R" />
			<item label="Log tkill ratings T" />
			<item label="Set signal V" />
			<item label="Toggle pause AI 'PAUSE'" />
			<item label="Kill all enemies 'BACKSLASH'" />
			<item label="  " />			
			<item label="Toggle wide view F1" />
			<item label="Toggle avatar fly fast F2" />
			<item label="Toggle avatar fly slow F3" />
			<item label="Screenshot F9" />
			<item label="GUI debug panel F10" />
		</item>
	</item>

	<item label="Switch Mode Selector Toolset" action="from gui.development.ui.mode_selector.mode_selector_toolset import showToolset; showToolset()"/>

	<item label="Change Window Size">
		<item label="Sizes" action="from gui.shared.utils.monitor_settings import g_monitorSettings as m; m.changeWindowSize">
			<param label="1024x768     Ctrl+1">(1024, 768)</param>
			<param label="1280x768     Ctrl+2">(1280, 768)</param>
			<param label="1366x768     Ctrl+3">(1366, 768)</param>
			<param label="1600x900     Ctrl+4">(1600, 900)</param>
			<param label="1920x1080   Ctrl+5">(1920, 1080)</param>
			<param label="2200x1200   Ctrl+6">(2200, 1200)</param>
			<param label="2560x1440   Ctrl+7">(2560, 1440)</param>
			<param label="3840x2160   Ctrl+8">(3840, 2160)</param>
		</item>
		<item label="Init hot keys Change Window Size manager" action="from development.as_tools.change_window_size_manager import g_change_manager as m; m.">
			<param label="start">start()</param>
			<param label="stop">stop()</param>
			<param label="Take screenShot or Ctrl+s">takeScreenShot()</param>
			<param label="Open screenShot folder or Ctrl+f">openFolder()</param>
		</item>
		<item label="Input folder name for screenShot" action="from development.as_tools.change_window_size_manager import g_change_manager as m; m.setFolderName" input="true" format="('%s')"/>
	</item>

	<item label="UI Logging">
		<item label="SF logger test dialog" action="from gui.development.ui import showSFUILoggerDemoView; showSFUILoggerDemoView()"/>
		<item label="GF logger test dialog" action="from gui.development.ui import showGFUILoggerDemoView; showGFUILoggerDemoView()"/>
	</item>

	<item label="WGCG Mock" action="from gui.development.ui import showWgcgMockView; showWgcgMockView()"/>

	<item label="Items">
		<item label="+1 recertification form" action="$p.inventory.addGoodie(goodieID=13461, amount=1)"/>
			<!-- game\res\wot\scripts\server_xml\goodies.xml <recertificationForm_13461> -->
		<item label="Add Equipment Item" action="$p.addEquipment" input="true" format="('%s')"/>
		<item label="Add Crew Book Item" action="$p.addCrewBooks" input="true" format="('%s')"/>
		<item label="Remove Equipment Item" action="$p.removeEquipment" input="true" format="('%s')"/>
	</item>

	<item label="PersonalMission">
		<item label="activate Personal Mission Season" action="$p.activatePersonalMissionsSeason">
			<param label="Season 1 and 2">((0, 2,), lambda x, y:(x, y))</param>
			<param label="Season 3">((4,), lambda x, y:(x, y))</param>
		</item>
		<item label="get PM3 rewards" action="$getPersonalMissionQuestRewards(" input="true" format="'%s')"/>
		<item label="complete Personal Mission with Additional" action="$p.stats.completePersonalMission(" input="true" format="%s, True)"/>
		<item label="complete Personal Mission without Additional" action="$p.stats.completePersonalMission(" input="true" format="%s, False)"/>
		<item label="complete Personal Mission range with Additional" action="$p.stats.completePersonalMissionRange(" input="true" format="'%s', True)"/>
		<item label="complete Personal Mission range without Additional" action="$p.stats.completePersonalMissionRange(" input="true" format="'%s', False)"/>
		<item label="add season 1 Free Award Lists" action="$p.stats.addFreeAwardLists(" input="true" format="%s, 1)"/>
		<item label="add season 2 Free Award Lists" action="$p.stats.addFreeAwardLists(" input="true" format="%s, 2)"/>
		<item label="draw season 1 Free Award Lists" action="$p.stats.drawFreeAwardLists(" input="true" format="%s, 1)"/>
		<item label="draw season 2 Free Award Lists" action="$p.stats.drawFreeAwardLists(" input="true" format="%s, 2)"/>
		<item label="add Tokens pm:points" action="$p.stats.addTokens('pm:points'," input="true" format="%s)"/>
		<item label="add Tokens pm3:8" action="$p.stats.addTokens('pm3:8:points'," input="true" format="%s)"/>
		<item label="add Tokens pm3:9" action="$p.stats.addTokens('pm3:9:points'," input="true" format="%s)"/>
		<item label="add Tokens pm3:10" action="$p.stats.addTokens('pm3:10:points'," input="true" format="%s)"/>
	</item>
	<item label="Crew">
		<item label="earn all skills" action="$earnAllSkills"/>
		<item label="Add all unique tankmen" action="$p.inventory.addAllUniqueTankmen()"/>
		<item label="Empty barracks" action="$p.inventory.emptyBarracks()"/>
		<item label="Show exact loading tip" action="from helpers.tips import showExactTip; showExactTip" input="true" format="('%s')"/>
		<item label="Give all tankmen max XP" action="$p.inventory.giveAllTankmenMaxXp()"/>
		<item label="Fill all tankmen skills" action="$p.inventory.fillAllTankmenSkills()"/>
		<item label="Reset all tankmen skills" action="$p.inventory.resetAllTankmenSkills()"/>
		<item label="Output TTC logs" action="from constants import IS_DEVELOPMENT;from gui.shared.items_parameters import params; setattr(params, '_DO_TTC_LOG', not params._DO_TTC_LOG and IS_DEVELOPMENT)"/>
		<item label="Add mentoring license" action="$p.inventory.addMentoringLicense()"/>
	</item>

	<item label="Game Loading">
		<item label="Show debug window" action="from gui.development.ui import showGameLoadingDebugView; showGameLoadingDebugView()"/>
	</item>

	<item label="ReInit battle hints" action="$reInitBattleHintsData"/>

	<item label="Environment Switching">
		<item label="Change battle environment" action="$p.base.setDevelopmentFeature" input="true" format="(0, 'changeEnvironment', 0, '%s')"/>
	</item>

	 <item label="Sounds">
		<item label="Debug Messages">
			<item label="Events" action="import WWISE; WWISE.WW_switchDebugMessagesType(1)"/>
			<item label="Switches" action="import WWISE; WWISE.WW_switchDebugMessagesType(2)"/>
			<item label="States" action="import WWISE; WWISE.WW_switchDebugMessagesType(4)"/>
		</item>
		<item label="Enable VO logging" action="from gui import IngameSoundNotifications; IngameSoundNotifications._ENABLE_VO_LOGS = True">
		</item>
		<item label="Play VO" action="from gui.battle_control.avatar_getter import getSoundNotifications; getSoundNotifications().play" input="true" format="('%s')"/>
	</item>

</root>
