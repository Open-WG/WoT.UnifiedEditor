<root>
	<strip_metadata>
		<rule>//metaData</rule>
	</strip_metadata>
	<strip_editorOnly>
		<rule>//editorOnly</rule>
	</strip_editorOnly>
	<default_vehicles>
		<create>
			<path>//chassis//(hysteresisHealth div healthRegenPerSec)</path>
			<name>repairTime</name>
		</create>
		<rule>//xmlns:xmlref</rule>
		<rule>//xpFactor</rule>
		<rule>//freeXpFactor</rule>
		<rule>//creditsFactor</rule>
		<rule>//fireStoppingChance</rule>
		<rule>//armorHomogenization</rule>
		<rule>//hysteresisHealth</rule>
		<rule>//healthRegenPerSec</rule>
		<rule>//healthBurnPerSec</rule>
		<rule>//physics/*[not(name()='detailed')]</rule>
		<rule>//physics/detailed/*[not(name()='engines') and not(name()='chassis')]</rule>
		<rule>//physics/detailed/engines/*[not(smplEnginePower) and not(name()='smplFwMaxSpeed')]</rule>
		<rule>//physics/detailed/engines/*/*[not(name()='smplEnginePower') and not(name()='smplFwMaxSpeed')]</rule>
		<rule>//physics/detailed/chassis/*[not(grounds) and not(axleCount) and not(axleSteeringAngles) and not(axleSteeringLockAngles) and not(wheelRiseSpeed) and not(isWheeledOnSpotRotation) and not(burnout)]</rule>
		<rule>//physics/detailed/chassis/*/*[not(name()='grounds') and not(name()='axleCount') and not(name()='axleSteeringAngles') and not(name()='axleSteeringLockAngles') and not(name()='wheelRiseSpeed') and not(name()='isWheeledOnSpotRotation') and not(name()='burnout')]</rule>
		<rule>//physics/detailed/chassis/*/grounds/*/*[not(name()='rollingFriction')]</rule>
		<rule>//physics/detailed/chassis/*/burnout/*</rule>
	</default_vehicles>
	<tank_components>
		<rule>//hysteresisHealth</rule>
		<rule>//healthRegenPerSec</rule>
		<rule>//healthBurnPerSec</rule>
	</tank_components>
	<shells>
		<rule>//normalizationAngle</rule>
		<rule>//ricochetAngle</rule>
	</shells>
	<vehicle>
		<rule>//balance</rule>
	</vehicle>
	<terrain>
		<rule>//terrain</rule>
	</terrain>
	<entity_def>
		<rule>//ServerOnlyEntities</rule>
	</entity_def>
	<arena_def_list>
		<rule>//map[$CORRECT_SPACES and translate(isDevelopment/text(), 'TRUE ', 'true')='true']</rule>
	</arena_def_list>
	<sequence_editor>
		<rule>//rotationEuler</rule>
		<rule>//editorOnly</rule>
	</sequence_editor>
</root>